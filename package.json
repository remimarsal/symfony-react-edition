{
  "name": "symfony-react-edition",
  "version": "0.0.1",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git://github.com/remimarsal/symfony-react-edition.git"
  },
  "browserify": {
    "transform": [
      "reactify",
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "jquery": "global:$",
    "react": "global:React",
    "react-router": "global:ReactRouter",
    "underscore": "global:_"
  },
  "devDependencies": {
    "browserify": "^9.0.3",
    "browserify-shim": "^3.8.3",
    "grunt": "^0.4.5",
    "grunt-bowercopy": "^1.2.0",
    "grunt-browserify": "^3.4.0",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-copy": "^0.8.0",
    "grunt-contrib-uglify": "^0.9.1",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-shell": "^1.1.1",
    "livereload-js": "^2.2.2"
  },
  "dependencies": {
    "jquery": "^2.1.3",
    "react": "^0.12.2",
    "react-router": "^0.12.0",
    "react-tap-event-plugin": "^0.1.4",
    "reactify": "^1.0.0",
    "reflux": "^0.2.5",
    "sassify": "^0.5.2",
    "superagent": "^1.1.0",
    "underscore": "^1.8.2"
  }
}
